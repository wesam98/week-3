
#include <stdio.h>
#include <stdlib.h>
void swap (int x,int y);
int main()
{ int num1,num2;
     printf("enter two numbers ");
     scanf("%d %d",&num1,&num2);
     printf("numbers before swapping are %d %d ""\n",num1,num2);
     swap(num1,num2);



return 0;
}
void swap (int x,int y)
{
    int c=x;
    x=y;
    y=c;
printf("two numbers after seapping are %d %d",x,y);
}
