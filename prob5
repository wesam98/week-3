

#include <stdio.h>
#include <stdlib.h>
int fun (int n);
int main()
{ int n;
 printf("enter any number ");
 scanf("%d",&n);
 printf("the fibonacci of the number is %d ",fun(n));
    return 0;
}
int fun (int n)
{ int k=0;
    if(n==1)
   return 1;
else
   {
       for(int i=n-1;i>=1;i--)
       {
           k+= fun(i);
       }
       return k;
   }



}


#include <stdio.h>
#include <stdlib.h>
int fun (int n);
int main()
{ int n;
 printf("enter any number ");
 scanf("%d",&n);
 printf("the fibonacci of the number is %d ",fun(n));
    return 0;
}
int fun (int n)
{ int k=0;
    if(n==1)
   return 1;
else
   {
       for(int i=n-1;i>=1;i--)
       {
           k+= fun(i);
       }
       return k;
   }



}
