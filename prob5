#include <stdio.h>
#include <stdlib.h>
int fun (int n);
int main()
{int num;
    printf("enter your number\n");
    scanf("%d",&num);
    printf("fibonacci of %d is %d" ,num,fun(num));
    return 0;
}
int fun (int n)
{
    if(n==0)
        return 0;
    else if(n==1)
        return 1;
    else
        return (fun(n-1)+fun(n-2));
}
